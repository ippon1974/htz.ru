ErrorDocument 404 http://www.htz.ru/error404.html
AddType application/x-httpd-php5 .php

RewriteEngine on
Options +FollowSymlinks
RewriteBase /news/item
RewriteRule ^4/([^/]*)\.html$ /news/item/?id=$1 [L]
RewriteRule ^([^/]*)/month/([^/]*)\.html$ /news/?year=$1&month=$2 [L]
RewriteRule (.+)(/)$ $1.html [L,QSA]









